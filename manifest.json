{
  "manifest_version": 2,
  "name": "ExploreCarta",
  "version": "1.0",

  "description": "Directly navigate to Carta from ExploreCourses.",
  "author": "Kashif Nazir",

  "web_accessible_resources": ["res/icon.png"],

  "browser_action": {
    "default_icon": "res/icon.png"
  },

  "content_scripts": [
    {
      "matches": [
        "http://explorecourses.stanford.edu/search*",
        "https://explorecourses.stanford.edu/search*"
      ],
      "js": ["plugin.js"],
      "css": ["main.css"]
    }
  ]
}